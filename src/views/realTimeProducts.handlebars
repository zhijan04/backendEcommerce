
<h1>Productos en Tiempo Real</h1>

<ul id="realTimeProducts">
    {{#each products}}
        <li id="product-{{this.id}}">{{products.title}} - {{this.description}} - ${{this.price}}</li>
        {{log this}}
    {{/each}}
</ul>

<script src="socket.io/socket.io.js"></script>
<script src='../public/js/products.js'></script>